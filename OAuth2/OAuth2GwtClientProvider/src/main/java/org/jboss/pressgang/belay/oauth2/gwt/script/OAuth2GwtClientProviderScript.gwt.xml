<module>
    <inherits name="org.jboss.pressgang.belay.oauth2.gwt.OAuth2GwtClientProvider"/>

    <inherits name="com.google.gwt.user.User"/>

    <entry-point class="org.jboss.pressgang.belay.oauth2.gwt.script.client.OAuth2GwtClientProviderScriptEntryPoint"/>

    <add-linker name="sso"/>

    <!-- Ignore runtime warnings to minimize output code size. -->
    <set-property name="compiler.stackMode" value="strip"/>
    <set-configuration-property name="document.compatMode.severity" value="IGNORE"/>
    <set-configuration-property name="user.agent.runtimeWarning" value="false"/>
</module>
